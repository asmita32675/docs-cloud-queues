     <!--
          Faults on Post Message 
      -->
     <!ENTITY postMessageFaults
           '
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post more than 100 messages with a single request.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post with request body greater than 4 KB.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">The post request included an invalid JSON body.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">The post request included a non-JSON body.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post with a non-JSON message. </doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post with a TTL less than 60.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post with a TTL greater than 1209600.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post with a non-integer value for TTL.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post with a negative value for TTL.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post without TTL in the request body.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post without the body parameter in request body.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post with no request body.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">An attempt was made to post with messages encapsulated in multiple arrays.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="400">
			<doc title="Bad request" xml:lang="EN">The request header has missing fields.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="401">
			<doc title="Unauthorized" xml:lang="EN">The request header has an invalid auth token.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="404">
			<doc title="Not found" xml:lang="EN">An attempt was made to post a message to a non-existing queue.</doc>
			<representation mediaType="application/json"/>
		</response> 
		<response status="406">
			<doc title="Not acceptable" xml:lang="EN">The request header has Accept !="application/json".</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="429">
			<doc title="Too many requests" xml:lang="EN">Too many requests.</doc>
			<representation mediaType="application/json"/>
		</response>
		<response status="503">
			<doc title="Service unavailable" xml:lang="EN">An attempt was made to post multiple messages in a single request and all of them failed.</doc>
			<representation mediaType="application/json"/>
		</response> 
